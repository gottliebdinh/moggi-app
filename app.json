{
  "expo": {
    "name": "MOGGI",
    "slug": "moggi-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "moggiapp",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "hideAsync": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moggi.app",
      "buildNumber": "6",
      "infoPlist": {
        "NSApplePayMerchantIdentifier": "merchant.com.moggi.app",
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Die Kamera wird benötigt, um QR-Codes zu scannen und Fotos hochzuladen.",
        "NSPhotoLibraryUsageDescription": "Wir benötigen Zugriff auf deine Fotobibliothek, um Bilder auszuwählen.",
        "NSLocationWhenInUseUsageDescription": "Dein Standort wird verwendet, um das nächste MOGGI Restaurant zu finden."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appLogo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.moggi.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "008c582f-cd4c-4c01-b15b-1b2fbf5c7022"
      }
    }
  }
}
